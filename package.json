{
  "name": "content-build",
  "version": "1.0.0",
  "description": "Content build for vets.gov website static assets.",
  "scripts": {
    "build": "node --max-old-space-size=5000 --expose-gc script/build-content.js",
    "build:compare": "node script/content-build-compare.js",
    "build:validate": "node ./script/validate-content-builds.js",
    "build:webpack": "webpack --config config/webpack.config.js",
    "build:codespaces": "./.devcontainer/codespaces-create.sh",
    "cy:open": "cypress open --config-file config/cypress.json",
    "cy:report:generate": "marge index.json --reportDir public --assetsDir public/assets --reportPageTitle index.html",
    "cy:report:merge": "mochawesome-merge cypress/results/json/*.json > index.json",
    "cy:run": "cypress run --config-file config/cypress.json",
    "cy:test:docker": "./script/run-cypress-tests-docker.sh",
    "fetch-drupal-cache": "node script/drupal-aws-cache.js --fetch",
    "prebuild": "node script/prebuild.js",
    "preview": "node script/preview.js",
    "heroku-serve": "http-server",
    "heroku-postbuild": "sh script/heroku-postbuild.sh",
    "install-repos": "./script/install-repos.sh",
    "lint": "npm run lint:js",
    "lint:changed": "npm run lint:js:changed",
    "lint:js": "eslint --quiet --ext .js --ext .jsx .",
    "lint:circle": "eslint --ext .js --ext .jsx -c circle.eslint.json .",
    "lint:js:fix": "eslint --fix --quiet --ext .js --ext .jsx .",
    "lint:js:changed": "LIST=`git diff-index --diff-filter=d --name-only --cached HEAD | grep \"\\.js[x]\\{0,1\\}$\"`; if [ \"$LIST\" ]; then eslint --quiet --config changed.eslintrc.js $LIST; fi",
    "lint:js:changed:fix": "LIST=`git diff-index --name-only HEAD | grep \"\\.js[x]\\{0,1\\}$\"`; if [ \"$LIST\" ]; then eslint --fix --quiet $LIST; fi",
    "pr-check": "node script/pr-check.js",
    "security-check": "node ./script/security-check.js",
    "serve": "node script/serve.js",
    "test": "npm run test:unit",
    "test:coverage": "./script/run-coverage.sh",
    "test:coverage-apps": "npm run test:coverage && node script/app-coverage-report.js",
    "test:puppeteer": "./script/run-puppeteer-tests.sh",
    "test:unit": "node ./script/run-unit-test.js",
    "test:watch": "node script/watch-tests.js",
    "update:schema": "./script/update-json-schema.sh",
    "watch": "concurrently \"node --max-old-space-size=5000 --expose-gc script/build-content.js --watch\" \"http-server build/localhost -s -c-1 -p 3002\"",
    "watch:codespaces": "concurrently \"yarn build:codespaces --watch\" \"http-server build/localhost -s -c-1 -p 3002\"",
    "watch:css-sourcemaps": "node --max-old-space-size=5000 --expose-gc script/build-content.js --watch --local-css-sourcemaps",
    "watch:review": "node ./script/run-review-instance-api.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/department-of-veterans-affairs/content-build.git"
  },
  "bugs": {
    "url": "https://github.com/department-of-veterans-affairs/content-build/issues"
  },
  "lint-staged": {
    "*.(js|jsx)": "eslint --fix"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/platform/testing/**",
      "**/*.spec.js",
      "**/*.spec.jsx"
    ]
  },
  "homepage": "https://github.com/department-of-veterans-affairs/content-build#readme",
  "browserslist": [
    "Chrome 60",
    "Firefox 56",
