{% comment %}
  Example data:
  entity: {
      fieldWysiwyg: {
          entityBundle: "wysiwyg",
          entityType: "paragraph",
          processed: 'Test'
      }
  }
{% endcomment %}

<div data-template="paragraphs/wysiwyg" data-entity-id="{{ entity.entityId }}">
  <div itemprop="text">
    {% if entity.fieldWysiwyg.processed %}
      {{ entity.fieldWysiwyg.processed | drupalToVaPath | phoneLinks }}
    {% elsif entity.fieldWysiwyg.0.processed %}
      {{ entity.fieldWysiwyg.0.processed | drupalToVaPath | phoneLinks }}
    {% endif %}
  </div>
</div>
